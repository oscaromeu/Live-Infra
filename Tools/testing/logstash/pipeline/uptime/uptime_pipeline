input {
  exec { command => "uptime" interval => 300 }
}

filter {
}


output {

  elasticsearch {
    hosts => ["https://es-master-headless:9200"]
    cacert => "/usr/share/logstash/config/certs/elastic-certificate.crt"
    user => '${ELASTICSEARCH_USERNAME}'
    password => '${ELASTICSEARCH_PASSWORD}'
    index => "uptime"
  }

}


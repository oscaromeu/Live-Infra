input {
  exec { command => "uptime" interval => 300 }
}

filter {
}


output {

  elasticsearch {
    hosts => ["https://elk-es-http.logging.svc:9200"]
    cacert => "/usr/share/logstash/config/certs/ca.crt"
    user => '${ELASTICSEARCH__NAME}'
    password => '${ELASTICSEARCH__PASSWORD}'
    index => "uptime"
  }

}

